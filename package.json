{
  "name": "expand-pkg",
  "description": "Normalize values in package.json.",
  "version": "0.3.1",
  "homepage": "https://github.com/jonschlinkert/expand-pkg/",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/expand-pkg",
  "bugs": {
    "url": "https://github.com/jonschlinkert/expand-pkg/issues"
  },
  "license": "MIT",
  "files": [
    "cli.js",
    "index.js",
    "lib"
  ],
  "engines": {
    "node": ">= 0.10.0"
  },
  "bin": {
    "expand-pkg": "cli.js"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "arr-union": "^3.1.0",
    "array-unique": "^0.2.1",
    "base": "^0.8.1",
    "base-data": "^0.4.0",
    "define-property": "^0.2.5",
    "export-files": "^2.1.1",
    "get-value": "^2.0.3",
    "git-user-name": "^1.2.0",
    "has-value": "^0.3.0",
    "is-primitive": "^2.0.0",
    "kind-of": "^3.0.2",
    "lazy-cache": "^1.0.3",
    "map-schema": "^0.2.0",
    "minimist": "^1.2.0",
    "mixin-deep": "^1.1.3",
    "omit-empty": "^0.3.3",
    "parse-author": "^0.2.1",
    "parse-git-config": "^0.4.0",
    "parse-github-url": "^0.3.0",
    "project-name": "^0.2.4",
    "remote-origin-url": "^0.5.0",
    "resolve": "^1.1.7",
    "semver": "^5.1.0",
    "set-value": "^0.3.3"
  },
  "devDependencies": {
    "gitty": "^3.3.4",
    "gulp": "^3.9.1",
    "gulp-eslint": "^2.0.0",
    "gulp-format-md": "^0.1.7",
    "gulp-istanbul": "^0.10.3",
    "gulp-mocha": "^2.2.0",
    "mocha": "^2.4.5"
  },
  "keywords": [
    "config",
    "fix",
    "json",
    "normalize",
    "package",
    "package-json",
    "pkg",
    "properties",
    "values"
  ],
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "update"
      ]
    },
    "reflinks": [
      "verb",
      "map-schema"
    ],
    "lint": {
      "reflinks": true
    }
  }
}