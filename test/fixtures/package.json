  "homepage": "https://github.com/jonschlinkert/expand-pkg/",
  "author": {
    "name": "Jon Schlinkert",
    "url": "https://github.com/jonschlinkert"
  },
  "repository": {
    "url": "https://github.com/jonschlinkert/expand-pkg.git"
  },
  "bugs": {
    "url": "https://github.com/jonschlinkert/expand-pkg/issues"
  },
  "license": "MIT",
  "files": [
